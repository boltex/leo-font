{
	"name": "product-icon-theme-sample",
	"version": "0.1.0",
	"publisher": "vscode-samples",
	"engines": {
		"vscode": "*"
	},
	"contributes": {
		"productIconThemes": [
			{
				"id": "vscode-v1-icons",
